{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "runtime_id",
    "port_mapping",
    "boot_mode",
    "sw_uploaded",
    "sw_commit",
    "sw_branch"
  ],
  "properties": {
    "runtime_id": {
      "type": "integer",
      "minimum": 0
    },
    "port_mapping": {
      "type": "object",
      "patternProperties": {
        "[0-9]+": {
          "type": "integer",
          "minimum": 0
        }
      }
    },
    "boot_mode": {
      "type": "integer"
    },
    "sw_uploaded": {
      "type": ["string", "null"]
    },
    "sw_commit": {
      "type": ["string", "null"]
    },
    "sw_branch": {
      "type": ["string", "null"]
    }
  }
}
