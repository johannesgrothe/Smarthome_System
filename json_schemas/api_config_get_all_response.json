  "type": "object",
  "required": ["configs"],
  "properties": {
    "configs": {
      "type": "object",
      "patternProperties": {
        "^[a-zA-Z_0-9]+$": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}